<%- model_class = Newdomain -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:domain_name) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%= model_class.human_attribute_name(:rank  ) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @newdomains.each do |newdomain| %>
      <tr>
        <td><%= link_to newdomain.id, newdomain_path(newdomain) %></td>
        <td><%= newdomain.domain_name %></td>
        <td><%=l newdomain.created_at %></td>
        <td><%= newdomain.rank %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      newdomain_path(newdomain), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_newdomain_path(newdomain), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      newdomain_path(newdomain),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_newdomain_path,
            :class => 'btn btn-primary' %>
